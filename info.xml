<?xml version="1.0"?>
<extension key="com.joineryhq.jentitylink" type="module">
  <file>jentitylink</file>
  <name>Entity Navigation Links</name>
  <description>Supports easy creation of custom action links in search results, contact actions menu, etc.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.jentitylink</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.jentitylink</url>
    <url desc="Support">https://github.com/JoineryHQ/com.joineryhq.jentitylink/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2023-08-18</releaseDate>
  <version>0.1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.58</ver>
  </compatibility>
  <comments>Developed by Joinery for CiviCRM</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Jentitylink</namespace>
    <format>23.02.1</format>
    <angularModule>crmJentitylink</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Jentitylink_Upgrader</upgrader>
</extension>
